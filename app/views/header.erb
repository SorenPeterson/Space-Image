<div>
  <a href="/images">Find</a>
  <a href="/images/new">Upload</a>
  <% if logged_in? %>
  <% else %>
    <span class="loginerrors"></span>
    <form class="login" action="users/login" method="post">
      <input type="text" name="user_name" placeholder="User Name">
      <input type="password" name="password" placeholder="email">
      <div class="button login">
        Log In
      </div>
    </form>
    <a href="/users/new">Sign Up</a>
  <% end %>
</div>
